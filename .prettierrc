{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": true,
  "bracketSpacing": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"]
}
